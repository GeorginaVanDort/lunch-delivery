<h1>Administrator</h1>

{{new-meal saveMeal="saveMeal"}}

{{#each model as |meal|}}
  <p>
    {{meal.name}}, Order Popularity:
    {{update-meal meal=meal updateMeal="updateMeal" destroyMeal="destroyMeal"}}
    <button {{action "destroyMeal" meal}} class="btn btn-danger">Delete Meal</button>
  </p>
{{/each}}
